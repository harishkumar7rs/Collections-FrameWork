package com.collections;

import java.util.Map;
import java.util.TreeMap;

public class Main {

    public static void main(String[] args) {
        TreeMap<Integer,String>treeMap=new TreeMap<>(Map.of(1,"a",2,"b",3,"S"));

        treeMap.put(4,"T");
        treeMap.put(5,"U");
        
        System.out.println(treeMap.ceilingEntry(2).getValue());
        System.out.println(treeMap.get(1));

        Map.Entry<Integer,String>entry=treeMap.firstEntry();
        System.out.println(entry);
        System.out.println(entry.getKey()+" "+entry.getValue());

        System.out.println(treeMap);

            }
}
//HashMap
package com.collections;

import java.util.HashMap;
import java.util.Map;
import java.util.TreeMap;

public class Main {

    public static void main(String[] args) {
        HashMap<Integer,String> hashMap=new HashMap<>(Map.of(1,"a",2,"b",3,"S"));

        hashMap.put(4,"T");
        hashMap.put(5,"U");

        System.out.println(hashMap.put(2,"B"));
        System.out.println(hashMap.entrySet());
        System.out.println(hashMap.get(1));



            }
}


